    <script type="text/javascript">
        function function2(id) {

            var values =
                           {
                               "Trainingid": id
                           }

            var opts =
                    {
                        "closeButton": true,
                        "debug": false,
                        "positionClass": "toast-top-right",
                        "toastClass": "White",
                        "onclick": null,
                        "showDuration": "300",
                        "hideDuration": "1000",
                        "timeOut": "2000",
                        "extendedTimeOut": "1000",
                        "showEasing": "swing",
                        "hideEasing": "linear",
                        "showMethod": "fadeIn",
                        "hideMethod": "fadeOut"
                    }


            // $('#portlet-config').appendTo("body").modal({ backdrop: 'static', keyboard: false });

            $.ajax({
                url: "../../Home/Mylearning/",
                type: 'POST',
                cache: false,
                data: values,
                traditional: true,
                global: false,
                success: function (result) {

                    if (result.Result === "Null") {
                        toastr.warning("Please Select The Trainee");
                        // $('#portlet-config').appendTo("body").modal('hide');
                    }
                    if (result.Result === "Instructor") {
                        $('#portlet-config').appendTo("body").modal('hide');
                        toastr.warning("Instructor does Not Allow To Nominate");
                    }
                    if (result.Result === "Already") {
                        bootbox.alert("You already nominated for the trainingid&nbsp;&nbsp;" + result.id + "&nbsp;&nbsp;");
                        // $('#portlet-config').appendTo("body").modal('hide');
                    }
                    if (result.Result === "NoSeat") {
                        toastr.warning("Seats not Available");
                        // $('#portlet-config').appendTo("body").modal('hide');
                    }
                    if (result.Result === "Success") {
                        toastr.success("Nomination added successfully", opts);
                        setTimeout(function () {
                            $('#portlet-config').appendTo("body").modal('hide');
                            window.location.reload(true);
                        }, 1000);
                    }

                }

            });
            // $('#portlet-config').appendTo("body").modal('hide');
        }

