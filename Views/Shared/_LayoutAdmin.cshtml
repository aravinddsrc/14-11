                                            }

                                            else
                                            {
                                                <i class="@module.MenuIcon"></i><span class="title">@module.MenuName</span>
                                            }

                                            @if (module.Children != null)
                                            {
                                                <span class="arrow"></span>
                                            }
                                            <span class="selected" />
                                        </a>
                                        @if (module.Children != null)
                                        {
                                            <ul class="sub-menu">
                                                @foreach (MenuListItem submodule in module.Children)
                                                {
                                                        
                                                    <li>
                                                        <a href="@Url.Content(submodule.Url)">

                                                            <i class="@submodule.MenuIcon"></i><span class="title">@submodule.MenuName</span>


                                                        </a>
                                                    </li>
                                                }
                                            </ul>
                                        }
