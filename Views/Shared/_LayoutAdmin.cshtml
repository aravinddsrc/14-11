                    foreach (MenuListItem function in Menu.Children)
                    {
                    <li class="root-level">20
                        @{
                         string url = function.Url;
                         if(function.Children!=null)
                         {
                            if(function.Children.Count == 1)
                            {
                                var child = function.Children.FirstOrDefault();
                                if(child!=null)
